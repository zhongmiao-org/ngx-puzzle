{
  "name": "@zhongmiao/ngx-puzzle",
  "author": "ark65<liuwufangzhou@gmail.com>,<liuwufangzhou@qq.vip.com>",
  "license": "MIT",
  "keywords": [
    "angular",
    "ngx",
    "dashboard",
    "drag-and-drop",
    "drag drop",
    "builder",
    "editor",
    "canvas",
    "grid",
    "echarts",
    "visualization",
    "data-visualization",
    "bi",
    "low-code",
    "library",
    "component",
    "signals",
    "standalone",
    "tethys",
    "chart",
    "table",
    "text",
    "control",
    "看板",
    "拖拽",
    "可视化",
    "仪表盘",
    "组件库",
    "图表",
    "表格",
    "文本",
    "控件"
  ],
  "peerDependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "rxjs": "~7.8.0",
    "zone.js": "~0.14.10"
  },
  "dependencies": {
    "@date-fns/tz": "^1.2.0",
    "@tethys/icons": "1.4.50",
    "@webdatarocks/webdatarocks": "1.4.19",
    "date-fns": "^4.1.0",
    "echarts": "6.0.0",
    "lodash": "4.17.21",
    "ngx-tethys": "^18.2.17",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@types/lodash": "4.17.20"
  },
  "exports": {
    ".": {
      "sass": "./styles/index.scss"
    },
    "./styles/variables": {
      "sass": "./styles/variables.scss"
    },
    "./styles/index": {
      "sass": "./styles/index.scss"
    },
    "./styles/*": {
      "sass": "./styles/*"
    }
  },
  "scripts": {
    "ng": "ng",
    "build:docs": "docgeni build",
    "start": "ng serve",
    "build": "npm run build:lib && node scripts/build-schematics.js && npm run build:styles && npm run build:pkgdocs",
    "build:lib": "ng build puzzle --configuration production",
    "build:styles": "cpx \"./projects/puzzle/src/**/*.scss\" dist/puzzle",
    "build:pkgdocs": "cpx \"CHANGELOG.md README.md LICENSE\" dist/puzzle",
    "start:docs": "docgeni serve --port 4600",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "release": "wpm release --release-branch-format release-auto-v{{version}}",
    "release:select": "node scripts/release.js -y",
    "prerelease": "npm run build",
    "changelog": "conventional-changelog -p conventionalcommits -r 1 -i CHANGELOG.md -s",
    "changelog:all": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s -r 0",
    "postrelease": "npm run changelog && git add CHANGELOG.md && git commit -m \"docs(changelog): update changelog\" || echo 'no changelog changes' && git push",
    "publish:dist": "cd dist/puzzle && npm publish --access public",
    "postinstall": "husky install"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
