{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "puzzle": {
      "projectType": "library",
      "root": "projects/puzzle",
      "sourceRoot": "projects/puzzle/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "tsConfig": "packages/gantt/tsconfig.lib.json",
            "project": "projects/puzzle/ng-package.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "./node_modules/@tethys/icons",
                "output": "/assets/icons/"
              }
            ],
            "styles": [
              "./node_modules/ngx-tethys/styles/index.scss"
            ]
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/puzzle/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/puzzle/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/puzzle/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "example": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "example",
      "sourceRoot": "example/src",
      "prefix": "example",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/example",
              "browser": ""
            },
            "index": "example/src/index.html",
            "polyfills": [
              "example/src/polyfills.ts"
            ],
            "tsConfig": "example/tsconfig.app.json",
            "assets": [
              "example/src/favicon.ico",
              "example/src/assets"
            ],
            "styles": [
              "example/src/styles.scss"
            ],
            "scripts": [],
            "extractLicenses": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true,
            "allowedCommonJsDependencies": [
              "docsearch.js"
            ],
            "browser": "example/src/main.ts"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "example/src/environments/environment.ts",
                  "with": "example/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "buildTarget": "example:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "example:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "example:build"
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "f10cc318-12df-411b-abbf-8de1ec4a0fa8"
  }
}
